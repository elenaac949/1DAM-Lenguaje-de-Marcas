                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="participante" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:attribute name="ref" type="xs:IDREF"></xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="sesiones">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="sesion" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:sequence>
