                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                            <xs:attribute name="id"></xs:attribute>
                                                            <xs:attribute name="tema" type="xs:string"></xs:attribute>
                                                            <xs:attribute name="fecha" type="xs:date"></xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="id"></xs:attribute>
                                    <xs:attribute name="nombre"></xs:attribute>
                                    <xs:attribute name="aÃ±o" type="xs:int"></xs:attribute>
                                    <xs:attribute name="ciudad" type="xs:string"></xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="personas">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="persona" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:attribute name="id" type="xs:ID"></xs:attribute>
                                    <xs:attribute name="nombre" type="xs:string"></xs:attribute>
                                    <xs:attribute name="afiliacion" type="xs:string"></xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>