<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="conferenciaAcademica">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="conferencias">
                    <xs:complexType>
